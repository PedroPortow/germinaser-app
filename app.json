{
  "expo": {
    "name": "GerminaSer",
    "slug": "client",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Nunito-Regular.ttf",
            "./src/assets/fonts/Nunito-Bold.ttf",
            "./src/assets/fonts/Nunito-SemiBold.ttf",
            "./src/assets/fonts/Nunito-ExtraLight.ttf",
            "./src/assets/fonts/Nunito-Light.ttf",
            "./src/assets/fonts/Nunito-Medium.ttf"
          ]
        }
      ],
      "expo-secure-store"
    ],
    "splash": {
      "image": "./src/assets/germina.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "germinaser.com"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2c0548b4-1616-41de-b8fb-79b28719d7a7"
      }
    },
    "owner": "pedroportow",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2c0548b4-1616-41de-b8fb-79b28719d7a7",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}
